@model List<AirForceSchoolYelahanka.Web.ViewModel.Home.NoticeItem>

<div class="notice-board">
    <h3>Notice Board</h3>
    <ul>
        @foreach (var notice in Model)
        {
            <li>
                <strong>@notice.Title</strong><br />
                <small class="text-muted">@notice.Date.ToString("dd MMM yyyy")</small>
            </li>
        }
    </ul>
</div>
