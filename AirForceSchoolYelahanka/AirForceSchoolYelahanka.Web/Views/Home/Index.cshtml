@model Dictionary<string, string>

@{
    ViewData["Title"] = Model.GetValueOrDefault("Title", "Home");
}

<div class="text-center">
    <h1 class="display-4">@Model.GetValueOrDefault("Title", "Welcome")</h1>
    <h4>@Model.GetValueOrDefault("Subtitle", "Your school subtitle")</h4>
    <p>@Model.GetValueOrDefault("Description", "This is a CMS-driven homepage.")</p>
</div>
